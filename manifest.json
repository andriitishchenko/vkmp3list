{
  "name": "vkmp3list",
  "version": "1",
  "description": "allow to get list of mp3 files on vk.com",
  "minimum_chrome_version": "16.0.884",
  "permissions": ["downloads", "<all_urls>"],
  "browser_action": {"default_popup": "popup.html"},
  "manifest_version": 2,
  "browser_action": {
    "name": "Test",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },
  "content_scripts": [ {
  	"css": ["css.css"],
    "js": [ "jquery-2.1.3.min.js", "background.js" ],
    "matches": ["https://vk.com/audio?q=*"]
  }]
}
